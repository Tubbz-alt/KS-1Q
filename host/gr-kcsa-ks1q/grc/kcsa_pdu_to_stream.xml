<?xml version="1.0"?>
<block>
  <name>PDU to Stream</name>
  <key>kcsa_pdu_to_stream</key>
  <category>[KCSA]</category>
  <import>import kcsa</import>
  <make>kcsa.pdu_to_stream($decim, $filler)</make>
  <param>
    <name>Decimation</name>
    <key>decim</key>
    <value>1</value>
    <type>int</type>
  </param>
  <param>
    <name>Filler</name>
    <key>filler</key>
    <value>0x00</value>
    <type>raw</type>
  </param>
  <sink>
    <name>in</name>
    <type>complex</type>
  </sink> 
  <sink>
    <name>pdu</name>
    <type>message</type>
  </sink>
  <source>
    <name>data</name>
    <type>byte</type>
  </source>
  <source>
    <name>amp</name>
    <type>byte</type>
  </source>
</block>

